<App name="com.incesoft.smsbot" version_code="1">
  <Directory directory_name="layout">
    <File file_name="activity_preferences.xml">
      <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
    <TextView android:textAppearance="?android:textAppearanceMedium" android:id="@id/textView4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Personality"/>
    <RadioButton android:id="@id/rbPersonalitySuperBot" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:text="SuperBot"/>
    <TextView android:textAppearance="?android:textAppearanceMedium" android:id="@id/textView3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Privacy Settings"/>
    <TextView android:textAppearance="?android:textAppearanceSmall" android:textStyle="italic" android:id="@id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Uncheck items that your chatbot should not reveal."/>
    <CheckBox android:id="@id/cbPrivacyContacts" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Contacts" android:onClick="onPrivacyCheckbox"/>
    <CheckBox android:id="@id/cbPrivacyLocation" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:text="Location"/>
    <TextView android:textAppearance="?android:textAppearanceMedium" android:id="@id/textView2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Chat With"/>
    <RadioGroup android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <RadioButton android:id="@id/rbAnyone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Anyone"/>
        <RadioButton android:id="@id/rbContacts" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:text="Contacts"/>
        <RadioButton android:id="@id/rbCustom" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Custom Whitelist"/>
    </RadioGroup>
</LinearLayout>
    </File>
    <File file_name="view_conversation.xml">
      <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
    <TextView android:textAppearance="?android:textAppearanceMedium" android:id="@id/cons_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Medium Text"/>
    <TextView android:textAppearance="?android:textAppearanceSmall" android:id="@id/cons_subtitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Small Text"/>
</LinearLayout>
    </File>
    <File file_name="activity_chat.xml">
      <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent" style="@xml/pref_headers">
    <TextView android:textAppearance="?android:textAppearanceMedium" android:gravity="center" android:id="@id/textView1" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Contact"/>
    <View android:background="#ff000000" android:layout_width="fill_parent" android:layout_height="2.0dip"/>
    <ScrollView android:layout_width="fill_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="vertical" android:id="@id/transcript" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    </ScrollView>
</LinearLayout>
    </File>
    <File file_name="activity_main.xml">
      <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
    <EditText android:id="@id/editFrom" android:layout_width="fill_parent" android:layout_height="wrap_content" android:hint="From" android:ems="10" android:layout_alignParentLeft="true" android:layout_alignParentTop="true"/>
    <EditText android:id="@id/editBody" android:layout_width="fill_parent" android:layout_height="wrap_content" android:hint="Message" android:ems="10"/>
    <Button android:id="@id/button1" android:layout_width="211.0dip" android:layout_height="wrap_content" android:text="Send SMS" android:onClick="onFakeSMS"/>
    <TextView android:textAppearance="?android:textAppearanceMedium" android:gravity="center" android:id="@id/chatText" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Conversations"/>
    <View android:background="#ff000000" android:layout_width="fill_parent" android:layout_height="2.0dip"/>
    <ListView android:id="@id/listView1" android:layout_width="fill_parent" android:layout_height="wrap_content"/>
</LinearLayout>
    </File>
  </Directory>
  <Directory directory_name="xml">
    <File file_name="pref_notification.xml">
      <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference android:title="New message notifications" android:key="notifications_new_message" android:defaultValue="true"/>
    <RingtonePreference android:title="Ringtone" android:key="notifications_new_message_ringtone" android:dependency="notifications_new_message" android:defaultValue="content://settings/system/notification_sound" android:ringtoneType="notification"/>
    <CheckBoxPreference android:title="Vibrate" android:key="notifications_new_message_vibrate" android:dependency="notifications_new_message" android:defaultValue="true"/>
</PreferenceScreen>
    </File>
    <File file_name="pref_headers.xml">
      <preference-headers xmlns:android="http://schemas.android.com/apk/res/android">
    <header android:title="General" android:fragment="${packageName}.${activityClass}$GeneralPreferenceFragment"/>
    <header android:title="Notifications" android:fragment="${packageName}.${activityClass}$NotificationPreferenceFragment"/>
    <header android:title="Data &amp; sync" android:fragment="${packageName}.${activityClass}$DataSyncPreferenceFragment"/>
</preference-headers>
    </File>
    <File file_name="pref_data_sync.xml">
      <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference android:entries="@array/pref_sync_frequency_titles" android:title="Sync frequency" android:key="sync_frequency" android:defaultValue="180" android:positiveButtonText="@null" android:negativeButtonText="@null" android:entryValues="@array/pref_sync_frequency_values"/>
    <Preference android:title="System sync settings">
        <intent android:action="android.settings.SYNC_SETTINGS"/>
    </Preference>
</PreferenceScreen>
    </File>
    <File file_name="pref_general.xml">
      <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference android:title="Enable social recommendations" android:key="example_checkbox" android:summary="Recommendations for people to contact based on your message history" android:defaultValue="true"/>
    <EditTextPreference android:maxLines="1" android:singleLine="true" android:selectAllOnFocus="true" android:capitalize="words" android:title="Display name" android:key="example_text" android:defaultValue="John Smith" android:inputType="textCapWords"/>
    <ListPreference android:entries="@array/pref_example_list_titles" android:title="Add friends to messages" android:key="example_list" android:defaultValue="-1" android:positiveButtonText="@null" android:negativeButtonText="@null" android:entryValues="@array/pref_example_list_values"/>
</PreferenceScreen>
    </File>
  </Directory>
  <Directory directory_name="menu">
    <File file_name="activity_preferences.xml">
      <menu xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/menu_settings" android:orderInCategory="100" android:title="Settings" android:showAsAction="never"/>
</menu>
    </File>
    <File file_name="activity_chat.xml">
      <menu xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/menu_settings" android:orderInCategory="100" android:title="Settings" android:showAsAction="never"/>
</menu>
    </File>
    <File file_name="activity_main.xml">
      <menu xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/menu_settings" android:orderInCategory="100" android:title="Settings" android:showAsAction="never"/>
</menu>
    </File>
  </Directory>
</App>
