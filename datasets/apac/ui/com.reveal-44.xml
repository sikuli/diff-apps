            <CheckBoxPreference android:title="Custom IP range" android:key="ip_custom" android:summary="Manually set start/end IPs" android:defaultValue="false"/>
            <EditTextPreference android:title="IP start" android:key="ip_start" android:summary="First IP address to scan" android:dependency="ip_custom" android:inputType="phone"/>
            <EditTextPreference android:title="IP end" android:key="ip_end" android:summary="Last IP address to scan" android:dependency="ip_custom" android:inputType="phone"/>
            <CheckBoxPreference android:title="Custom CIDR" android:key="cidr_custom" android:summary="Manually set CIDR (defines subnet)" android:defaultValue="false"/>
            <EditTextPreference android:title="Change CIDR" android:key="cidr" android:summary="Change CIDR" android:dependency="cidr_custom" android:inputType="number"/>
            <CheckBoxPreference android:title="Rate control" android:key="ratecontrol_enable" android:summary="Enable automatic rate detection" android:defaultValue="true"/>
            <EditTextPreference android:title="Custom rate" android:key="timeout_discover" android:summary="Rate of discovery in millisecondes" android:defaultValue="500" android:inputType="number"/>
