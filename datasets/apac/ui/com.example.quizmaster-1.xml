  <Directory directory_name="xml">
    <File file_name="sample.xml">
      <quiz>
    <title>Sample Quiz</title>
    <config>
        <length>5</length>
        <time>30</time>
        <choices>5</choices>
    </config>
    <questions>
        <question>
            <prompt>India is a subcontinent in _____.</prompt>
            <incorrect>Africa</incorrect>
            <incorrect>the Middle East</incorrect>
            <answer>Asia</answer>
            <incorrect>the Atlantic Ocean</incorrect>
            <incorrect>Great Britain</incorrect>
        </question>
        <question>
            <prompt>Mexico is a country in which continent?</prompt>
            <incorrect>United States</incorrect>
            <incorrect>South America</incorrect>
            <incorrect>Africa</incorrect>
            <incorrect>Texas</incorrect>
            <answer>North America</answer>
        </question>
        <question>
            <prompt>Penguins are found in _____.</prompt>
            <incorrect>the Arctic</incorrect>
            <answer>the Antarctic</answer>
            <incorrect>igloos</incorrect>
        </question>
        <question>
            <prompt>Alaska is a state in Canada.</prompt>
            <incorrect>True</incorrect>
            <answer>False</answer>
            <choices>2</choices>
        </question>
        <question>
            <prompt>The capital of the United States of America is _____.</prompt>
            <answer>Washington, DC</answer>
            <incorrect>New York, NY</incorrect>
            <incorrect>Los Angeles, CA</incorrect>
            <incorrect>San Francisco, CA</incorrect>
            <incorrect>Miami, FL</incorrect>
        </question>
        <question>
            <prompt>This is the only island that is also a continent.</prompt>
            <incorrect>What is Japan?</incorrect>
            <incorrect>What is Hawaii?</incorrect>
            <incorrect>What is Singapore?</incorrect>
            <answer>What is Australia?</answer>
        </question>
        <question>
            <prompt>A train leaves from Tokyo. It is heading to _____.</prompt>
            <answer>Kyoto</answer>
        </question>
        <question>
            <prompt>Which country is not located in Africa?</prompt>
            <incorrect>Egypt</incorrect>
            <incorrect>Côte d'Ivoire</incorrect>
            <incorrect>Somalia</incorrect>
            <incorrect>Sierra Leone</incorrect>
            <answer>None of the above</answer>
            <choices>5</choices>
        </question>
    </questions>
</quiz>
    </File>
    <File file_name="settings.xml">
      <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="Quiz Settings">
        <CheckBoxPreference android:title="Randomize question order" android:key="random_question_order" android:defaultValue="false" android:summaryOn="Questions will be shuffled" android:summaryOff="Questions will be shown in order"/>
        <CheckBoxPreference android:title="Randomize answer order" android:key="random_answer_order" android:defaultValue="false" android:summaryOn="Answers will be shuffled" android:summaryOff="Answers will be shown in order"/>
        <CheckBoxPreference android:title="Fill choices with random answers" android:key="random_fill_choices" android:defaultValue="false" android:summaryOn="Extra choices will be filled from other questions" android:summaryOff="Questions will not have extra choices"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="Score Submission">
        <CheckBoxPreference android:title="Submit scores to server" android:key="submit_scores" android:defaultValue="false"/>
        <EditTextPreference android:title="Server URL" android:key="submit_server" android:dependency="submit_scores" android:defaultValue="http://www.example.com/quiz"/>
        <CheckBoxPreference android:title="Show high scores" android:key="highscores" android:summary="Retrieve and show high scores at the end of a quiz" android:dependency="submit_scores" android:defaultValue="true"/>
    </PreferenceCategory>
</PreferenceScreen>
