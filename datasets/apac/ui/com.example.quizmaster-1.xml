    <File file_name="sample_math.xml">
      <quiz>
    <title>Math Quiz</title>
    <config>
        <length>5</length>
        <time>25</time>
        <choices>5</choices>
    </config>
    <questions>
        <question>
            <prompt>1 + 1 = ?</prompt>
            <incorrect>0</incorrect>
            <incorrect>1</incorrect>
            <answer>2</answer>
            <incorrect>3</incorrect>
            <incorrect>All of the above</incorrect>
        </question>
        <question>
            <prompt>4 ÷ 2 = ?</prompt>
            <incorrect>0</incorrect>
            <incorrect>1</incorrect>
            <answer>2</answer>
            <incorrect>4</incorrect>
            <incorrect>∞</incorrect>
        </question>
        <question>
            <prompt>√4 = ?</prompt>
            <incorrect>1</incorrect>
            <answer>2</answer>
            <incorrect>4</incorrect>
            <incorrect>8</incorrect>
            <incorrect>16</incorrect>
        </question>
        <question>
            <prompt>2¹ = ?</prompt>
            <incorrect>0</incorrect>
            <incorrect>1</incorrect>
            <answer>2</answer>
            <incorrect>4</incorrect>
            <incorrect>21</incorrect>
        </question>
        <question>
            <prompt>(2 × 2 ÷ 2 + 2)² = ?</prompt>
            <incorrect>2</incorrect>
            <incorrect>4</incorrect>
            <answer>16</answer>
            <incorrect>64</incorrect>
            <incorrect>256</incorrect>
        </question>
        <question>
            <prompt>2∫xdx = ?</prompt>
            <incorrect>0</incorrect>
            <incorrect>1</incorrect>
            <incorrect>2</incorrect>
            <incorrect>2x + c</incorrect>
            <answer>2x² + c</answer>
        </question>
    </questions>
</quiz>
