  <Directory directory_name="xml">
    <File file_name="preferences.xml">
      <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:key="Butane_prefs">
    <CheckBoxPreference android:title="On Call" android:key="is_oncall" android:summary="Are you oncall?" android:defaultValue="true"/>
    <RingtonePreference android:title="Alert Sound" android:key="alert_sound" android:defaultValue="content://settings/system/notification_sound" android:ringtoneType="all" android:showDefault="true" android:showSilent="true"/>
    <CheckBoxPreference android:title="Use Alarm Volume?" android:key="use_alarm_stream" android:summary="use the alarm volume, instead of the ringer volume" android:defaultValue="false"/>
    <CheckBoxPreference android:title="Vibrate?" android:key="vibrate" android:defaultValue="true"/>
    <PreferenceScreen android:title="Sms Settings" android:key="message_prefs" android:summary="preferences for pages received by SMS">
        <PreferenceCategory android:title="Incoming Message Settings" android:key="receive settings">
            <EditTextPreference android:title="Sender" android:key="sender_match" android:summary="substring match on sender phone number or email" android:defaultValue=""/>
            <ListPreference android:entries="@array/pageparser" android:title="Page Parser Type" android:key="pageparser" android:defaultValue="Standard" android:entryValues="@array/pageparser"/>
            <CheckBoxPreference android:title="Consume SMS Message" android:key="consume_sms_message" android:summary="prevent the SMS from being delivered to other apps" android:defaultValue="false"/>
            <CheckBoxPreference android:title="Also Match Message Body" android:key="also_match_body" android:summary="match sender pattern against the body of the message" android:defaultValue="false"/>
        </PreferenceCategory>
        <PreferenceCategory android:title="Reply Settings" android:key="reply settings">
            <CheckBoxPreference android:title="Include Subject Line" android:key="include_subject" android:summary="include the subject line in parenthesis in reply." android:defaultValue="false"/>
            <CheckBoxPreference android:title="Use Received Service Center" android:key="use_received_service_center" android:summary="use the SMS Service Center from the received message?" android:defaultValue="false"/>
            <CheckBoxPreference android:title="Send Destination" android:key="include_dest_address" android:summary="Some SMS gateways require that the destination address be specified" android:defaultValue="false"/>
        </PreferenceCategory>
    </PreferenceScreen>
    <Preference android:title="Edit Replies" android:key="edit_replies" android:summary="edit the text of quick responses"/>
    <ListPreference android:entries="@array/intervals" android:title="Notification Interval" android:key="notification_interval" android:summary="Notification Interval" android:defaultValue="20000" android:entryValues="@array/intervals_in_ms"/>
    <Preference android:title="Changelog" android:key="changelog" android:summary="notable changes between releases"/>
    <Preference android:title="Version" android:key="version"/>
</PreferenceScreen>
