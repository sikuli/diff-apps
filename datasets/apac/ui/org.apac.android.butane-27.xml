    <EditText android:textSize="20.0px" android:textStyle="bold" android:id="@id/subject" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="reply title" android:layout_weight="1.0" android:layout_toRightOf="@id/icon"/>
