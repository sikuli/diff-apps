                <PreferenceScreen android:title="Default Notifications" android:key="pref_notif_default_key" android:summary="Notifications for all contacts without an individual customization">
                    <PreferenceCategory android:title="Default Notifications">
                        <CheckBoxPreference android:title="Show a Popup" android:key="popup" android:defaultValue="true" android:summaryOn="Message will open in a popup" android:summaryOff="Message will only show in notification area"/>
