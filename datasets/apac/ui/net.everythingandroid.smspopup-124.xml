                        <RingtonePreference android:title="Notification Sound" android:key="notifsound" android:summary="This sound will play when a message is received" android:dependency="notification" android:defaultValue="content://settings/system/notification_sound" android:ringtoneType="notification" android:showDefault="true" android:showSilent="true"/>
                        <CheckBoxPreference android:title="Notify On Call" android:key="notifyoncall" android:dependency="notification" android:defaultValue="false" android:summaryOn="Sound will play while on a call" android:summaryOff="Sound will not play while on a call"/>
                        <net.everythingandroid.smspopup.preferences.NotificationIconListPreference android:entries="@array/pref_notif_icon_entries" android:title="Notification Icon" android:key="notificon" android:summary="Change the notification area icon" android:dependency="notification" android:defaultValue="0" android:entryValues="@array/pref_notif_icon_values"/>
                        <net.everythingandroid.smspopup.preferences.TestNotificationDialogPreference android:title="Test Notification" android:key="notiftest" android:summary="Test out your notification" android:dependency="notification" android:dialogTitle="Test Notification" android:dialogMessage="Testing your notification now!" android:dialogIcon="@android:drawable/ic_dialog_alert" android:negativeButtonText="@null"/>
                    </PreferenceCategory>
                    <PreferenceCategory android:title="Vibrate Configuration">
                        <CheckBoxPreference android:title="Vibrate" android:key="vibrate" android:dependency="notification" android:defaultValue="true" android:summaryOn="Vibrate is on" android:summaryOff="Vibrate is off"/>
                        <net.everythingandroid.smspopup.preferences.CustomVibrateListPreference android:entries="@array/pref_vibrate_pattern_entries" android:title="Vibrate Pattern" android:key="vibrate_pattern" android:summary="Set the vibration pattern" android:dependency="vibrate" android:defaultValue="0,1200" android:entryValues="@array/pref_vibrate_pattern_values"/>
                    </PreferenceCategory>
                    <PreferenceCategory android:title="LED Configuration">
                        <CheckBoxPreference android:title="Blink LED" android:key="flashled" android:dependency="notification" android:defaultValue="true" android:summaryOn="Blink LED when a message is received" android:summaryOff="Do not blink LED when a message is received"/>
                        <net.everythingandroid.smspopup.preferences.CustomLEDColorListPreference android:entries="@array/pref_led_color_entries" android:title="LED Color" android:key="flashled_color" android:summary="The color of the LED" android:dependency="flashled" android:defaultValue="Yellow" android:entryValues="@array/pref_led_color_values"/>
                        <net.everythingandroid.smspopup.preferences.CustomLEDPatternListPreference android:entries="@array/pref_led_pattern_entries" android:title="LED Blink Rate" android:key="flashled_pattern" android:summary="Set the speed the LED blinks" android:dependency="flashled" android:defaultValue="1000,1000" android:entryValues="@array/pref_led_pattern_values"/>
                    </PreferenceCategory>
                    <PreferenceCategory android:title="Reminder Configuration">
                        <CheckBoxPreference android:title="Enable Reminders" android:key="notif_repeat" android:defaultValue="false" android:summaryOn="Unread message reminders are enabled" android:summaryOff="Unread message reminders are disabled"/>
