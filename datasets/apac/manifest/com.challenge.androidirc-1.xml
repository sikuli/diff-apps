<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.challenge.androidirc" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.challenge.androidirc.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service android:label="Connection Service" android:name=".ConnectionService"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="11"/></manifest>
