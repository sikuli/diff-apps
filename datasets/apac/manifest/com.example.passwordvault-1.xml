        <activity android:label="@string/title_activity_export_vault" android:name="com.example.passwordvault.ExportVaultActivity" android:noHistory="true"/>
        <activity android:label="@string/title_activity_import_file_list" android:name="com.example.passwordvault.ImportFileListActivity" android:noHistory="true"/>
