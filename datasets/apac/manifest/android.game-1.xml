<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="android.game" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".AndroidGameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="10"/></manifest>
