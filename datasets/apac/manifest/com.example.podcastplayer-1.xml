<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.podcastplayer" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/player_name" android:name=".PlayerActivity"/>
        <activity android:label="@string/title_activity_main" android:name=".FeedActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_main" android:name=".EntryListActivity"/>
        <service android:name=".MusicService"/>
        <receiver android:name=".DownloadReceiver"/>
    </application>
<uses-sdk android:targetSdkVersion="17" android:minSdkVersion="14"/></manifest>
