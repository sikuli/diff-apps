<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.incesoft.smsbot" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <receiver android:name="com.incesoft.smsbot.SmsReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
        <activity android:label="@string/title_activity_settings" android:name="com.incesoft.smsbot.SettingsActivity"/>
        <activity android:label="@string/title_activity_preferences" android:name="com.incesoft.smsbot.PreferencesActivity"/>
        <activity android:label="@string/title_activity_main" android:name="com.incesoft.smsbot.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_chat" android:name="com.incesoft.smsbot.ChatActivity"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15"/></manifest>
