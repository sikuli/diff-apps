<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.fillinfun" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:icon="@drawable/fif_launcher" android:label="FillInFun" android:theme="@style/AppTheme">
        <activity android:label="FillInFun" android:name=".MainActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_input" android:name=".InputActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter/>
        </activity>
        <activity android:label="@string/title_activity_result" android:name=".ResultActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter/>
        </activity>
        <activity android:label="@string/title_activity_unlock_templates" android:name=".UnlockTemplatesActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter/>
        </activity>
        <activity android:label="@string/title_activity_unlock_speech" android:name=".UnlockSpeechActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter/>
        </activity>
        <activity android:label="@string/title_activity_verify_speech" android:name=".VerifySpeechActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter/>
        </activity>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15"/></manifest>
