<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.apac.sentinel" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <application android:allowBackup="true" android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="com.apac.sentinel.App" android:theme="@android:style/Theme.Holo">
        <activity android:label="@string/title_activity_sentinel_home" android:name="com.apac.sentinel.gui.SentinelHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="com.apac.sentinel.gui.MainActivity"/>
        <activity android:label="@string/title_activity_lock" android:launchMode="singleInstance" android:name="com.apac.sentinel.gui.LockActivity"/>
        <activity android:label="@string/title_preferences" android:name="com.apac.sentinel.gui.Preferences"/>
        <activity android:label="@string/title_activity_manage_log" android:name="com.apac.sentinel.gui.ViewLogActivity"/>
        <activity android:label="@string/title_activity_view_log_entry" android:name="com.apac.sentinel.gui.ViewLogEntryActivity"/>
        <activity android:label="@string/title_activity_silent_image" android:name="com.apac.sentinel.gui.SilentImageActivity"/>
        <activity android:label="@string/title_activity_update_iccid" android:name="com.apac.sentinel.gui.UpdateICCIDActivity"/>
        <provider android:authorities="com.apac.sentinel.logprovider" android:exported="false" android:name="com.apac.sentinel.LogProvider"/>
        <receiver android:name="com.apac.sentinel.SMSReceiver">
            <intent-filter android:priority="214783648">
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.apac.sentinel.BootCompletedReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <receiver android:description="@string/device_admin_description" android:name=".DeviceAdmin" android:permission="android.permission.BIND_DEVICE_ADMIN">
            <meta-data android:name="android.app.device_admin" android:resource="@xml/device_admin"/>
            <intent-filter>
                <action android:name="android.app.action.DEVICE_ADMIN_ENABLED"/>
            </intent-filter>
        </receiver>
        <service android:exported="false" android:name="com.apac.sentinel.TheftMonitorService"/>
    </application>
<uses-sdk android:targetSdkVersion="19" android:minSdkVersion="19"/></manifest>
