<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="org.darpa.supersudoku" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE"/>
    <application android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/title_activity_main_menu" android:name=".MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_new_puzzle" android:name=".NewPuzzle"/>
        <activity android:label="@string/title_activity_solve_puzzle" android:name=".SolvePuzzle"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15"/></manifest>
