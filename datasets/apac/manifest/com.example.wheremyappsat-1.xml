<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.wheremyappsat" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:debuggable="true" android:icon="@drawable/icon_android" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".ReadFileActivity"/>
        <activity android:label="@string/app_name" android:name=".ExplorerActivity">
            <intent-filter>
                <action android:name="com.example.wheremyappsat.action.SELECT_FILE"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="file"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.example.wheremyappsat.action.SELECT_FILE"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.example.wheremyappsat.action.SELECT_DIRECTORY"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="file"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.example.wheremyappsat.action.SELECT_DIRECTORY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.example.wheremyappsat.action.BACKUP"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="file"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.example.wheremyappsat.action.BACKUP"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15" android:maxSdkVersion="15"/></manifest>
