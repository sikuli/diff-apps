<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.invincea.screensaver" versionName="4.0" versionCode="2">
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <application android:debuggable="true" android:icon="@drawable/icon" android:label="Screensaver" android:logo="@drawable/icon">
        <activity android:label="ScreenSaver" android:name="com.invincea.draw.MyGLActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service android:enabled="true" android:name="com.invincea.draw.MyDrawService"/>
        <receiver android:name="com.invincea.draw.MyDrawService">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.invincea.draw.MyDrawService"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15"/></manifest>
