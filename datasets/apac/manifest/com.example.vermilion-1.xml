<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.vermilion" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application android:allowBackup="true" android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name=".ui.VermilionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.PreviewActivity"/>
        <activity android:name=".ui.SettingsActivity"/>
        <service android:enabled="true" android:name="com.example.vermilion.services.WifiService"/>
        <service android:enabled="true" android:name="com.example.vermilion.services.BluetoothService"/>
        <service android:enabled="true" android:name="com.example.vermilion.services.LocationService"/>
        <service android:enabled="true" android:name="com.example.vermilion.services.SignalService"/>
        <service android:enabled="true" android:name="com.example.vermilion.services.FaceFinderService"/>
        <service android:enabled="true" android:name="com.example.vermilion.services.FaceBlanker"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15"/></manifest>
