<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.deryl.wordhelper" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/title_activity_main" android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_used_spaces" android:name=".UsedSpacesActivity" android:screenOrientation="landscape"/>
        <activity android:label="@string/title_activity_tile_rack" android:name=".TileRackActivity" android:screenOrientation="portrait"/>
        <activity android:label="@string/title_activity_word_display" android:name=".WordDisplayActivity" android:screenOrientation="portrait"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="14"/></manifest>
