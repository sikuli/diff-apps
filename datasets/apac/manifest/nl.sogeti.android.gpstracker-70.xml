<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="nl.sogeti.android.gpstracker" versionName="1.3.2" versionCode="70">
    <supports-screens android:largeScreens="true" android:normalScreens="true" android:smallScreens="true"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-feature android:name="android.hardware.location.gps" android:required="true"/>
    <application android:allowClearUserData="true" android:debuggable="true" android:description="@string/app_name" android:hardwareAccelerated="true" android:icon="@drawable/icon" android:label="@string/app_name">
        <uses-library android:name="com.google.android.maps"/>
        <meta-data android:name="org.openintents.metadata.COMMENTS" android:value="@string/about_comments"/>
        <meta-data android:name="org.openintents.metadata.COPYRIGHT" android:value="@string/about_copyright"/>
        <meta-data android:name="org.openintents.metadata.WEBSITE_LABEL" android:value="@string/about_website_label"/>
        <meta-data android:name="org.openintents.metadata.WEBSITE_URL" android:value="@string/about_website_url"/>
        <meta-data android:name="org.openintents.metadata.EMAIL" android:value="@string/about_email"/>
        <meta-data android:name="android.app.default_searchable" android:value=".viewer.TrackList"/>
        <meta-data android:name="CLOUDMADE_KEY" android:value="534dfce474894e218f363a5473248ff6"/>
        <activity android:label="@string/app_name" android:launchMode="singleTask" android:name=".viewer.map.CommonLoggerMap">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter android:label="Show track on map">
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="content"/>
                <data android:host="nl.sogeti.android.gpstracker"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.nl.sogeti.android.track"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:launchMode="singleTask" android:name=".viewer.map.GoogleLoggerMap" android:theme="@style/Theme.NoBackground"/>
        <activity android:label="@string/app_name" android:launchMode="singleTask" android:name=".viewer.map.OsmLoggerMap" android:theme="@style/Theme.NoBackground"/>
        <activity android:label="@string/app_name" android:launchMode="singleTask" android:name=".viewer.map.MapQuestLoggerMap" android:theme="@style/Theme.NoBackground"/>
        <activity android:label="@string/menu_settings" android:name=".viewer.ApplicationPreferenceActivity"/>
        <activity android:name=".actions.NameTrack" android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
        <activity android:name=".actions.DescribeTrack" android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
        <activity android:name=".actions.ControlTracking" android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
        <activity android:name=".actions.InsertNote" android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
        <activity android:label="@string/track_list" android:launchMode="singleTop" android:name=".viewer.TrackList">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="file"/>
                <data android:mimeType="*/*"/>
                <data android:pathPattern=".*\\.gpx"/>
                <data android:host="*"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:mimeType="text/xml"/>
            </intent-filter>
            <meta-data android:name="android.app.searchable" android:resource="@xml/searchable"/>
        </activity>
        <activity android:icon="@drawable/icon" android:label="@string/track_list" android:name=".viewer.TracksLiveFolder">
            <intent-filter>
                <action android:name="android.intent.action.CREATE_LIVE_FOLDER"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/menu_statistics" android:name=".actions.Statistics"/>
        <activity android:label="@string/share_track" android:name=".actions.ShareTrack" android:windowSoftInputMode="stateUnchanged">
            <intent-filter>
                <action android:name="android.intent.action.RUN"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.nl.sogeti.android.track"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/authentication_redirect" android:launchMode="singleTask" android:name=".oauth.PrepareRequestTokenActivity" android:theme="@android:style/Theme.Dialog">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:host="callback" android:scheme="x-oauthflow-opengpstracker"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/menu_about" android:name=".viewer.About"/>
        <service android:enabled="true" android:name=".logger.GPSLoggerService" android:permission="android.permission.ACCESS_FINE_LOCATION">
            <intent-filter android:label="GPS logger">
                <action android:name="nl.sogeti.android.gpstracker.intent.action.GPSLoggerService"/>
            </intent-filter>
        </service>
        <provider android:authorities="nl.sogeti.android.gpstracker" android:enabled="true" android:name=".db.GPStrackingProvider" android:permission="android.permission.ACCESS_FINE_LOCATION">
            <path-permission android:path="/search_suggest_query" android:readPermission="android.permission.GLOBAL_SEARCH"/>
            <path-permission android:path="/live_folders/tracks" android:readPermission="android.permission.BIND_APPWIDGET"/>
        </provider>
        <receiver android:name=".util.BootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".util.DockReceiver">
            <intent-filter>
                <action android:name="android.intent.action.DOCK_EVENT"/>
            </intent-filter>
        </receiver>
        <receiver android:label="Logger control" android:name=".widget.ControlWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
            <intent-filter>
                <action android:name="nl.sogeti.android.gpstracker.LOGGING_STATE_CHANGED"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/control_appwidget_info"/>
        </receiver>
    </application>
<uses-sdk android:targetSdkVersion="4" android:minSdkVersion="7"/></manifest>
