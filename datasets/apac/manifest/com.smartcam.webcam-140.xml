<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.smartcam.webcam" versionName="1.4.0" versionCode="140">
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:icon="@drawable/smartcam" android:label="@string/app_name">
        <service android:exported="true" android:name="com.smartcam.webcam.comm.CommService"/>
        <activity android:label="@string/app_name" android:name="com.smartcam.webcam.ui.SmartCamActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.smartcam.webcam.ui.HelpActivity" android:screenOrientation="user">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/settings_name" android:name="com.smartcam.webcam.ui.SettingsActivity" android:screenOrientation="user"/>
        <meta-data android:name="ADMOB_PUBLISHER_ID" android:value="xxxxxxxxxxxxxxx"/>
    </application>
<uses-sdk android:minSdkVersion="8"/></manifest>
