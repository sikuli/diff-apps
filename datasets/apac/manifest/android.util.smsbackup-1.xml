<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="android.util.smsbackup" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="com.android.browser.permission.READ_HISTORY_BOOKMARKS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/title_activity_main" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Backup"/>
        <service android:name="MyService">
            <intent-filter>
                <action android:name="android.util.smsbackup.MyService"/>
            </intent-filter>
        </service>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="8"/></manifest>
