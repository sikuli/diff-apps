<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.expenses" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:allowBackup="true" android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@android:style/Theme.Holo">
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:label="@string/app_name" android:name=".ReportActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name=".ExpenseActivity" android:screenOrientation="portrait">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".ReportActivity"/>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name=".EditExpenseActivity" android:windowSoftInputMode="stateUnchanged">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".ExpenseActivity"/>
        </activity>
        <activity android:name=".ReceiptActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".EditExpenseActivity"/>
        </activity>
        <activity android:name=".ReportViewerActivity"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="14"/></manifest>
