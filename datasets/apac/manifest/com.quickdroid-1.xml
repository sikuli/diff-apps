<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.quickdroid" versionName="1.0.0" versionCode="1">
    <permission android:description="@string/access_bookmarks" android:label="Search bookmarks" android:name="com.quickdroid.perm.SEARCH_BOOKMARKS" android:permissionGroup="android.permission-group.PERSONAL_INFO" android:protectionLevel="dangerous"/>
    <permission android:description="@string/access_contact_names" android:label="Search contacts" android:name="com.quickdroid.perm.SEARCH_CONTACTS" android:permissionGroup="android.permission-group.PERSONAL_INFO" android:protectionLevel="dangerous"/>
    <permission android:description="@string/access_contact_phone_numbers" android:label="Search phone numbers" android:name="com.quickdroid.perm.SEARCH_CONTACT_PHONE_NUMBERS" android:permissionGroup="android.permission-group.PERSONAL_INFO" android:protectionLevel="dangerous"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="com.android.browser.permission.READ_HISTORY_BOOKMARKS"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT"/>
    <application android:icon="@drawable/android_with_white_skateboard" android:label="@string/appName" android:name="com.quickdroid.App">
        <activity android:clearTaskOnLaunch="true" android:label="@string/appName" android:launchMode="singleTask" android:name=".Quickdroid" android:windowSoftInputMode="stateVisible|adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEARCH_LONG_PRESS"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/appSettings" android:name=".Preferences"/>
        <receiver android:name=".AutoStarter">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <provider android:authorities="quickdroid" android:multiprocess="false" android:name=".apps.AppProvider"/>
        <service android:name=".apps.AppSyncer"/>
        <service android:name=".apps.AppQuery">
            <intent-filter>
                <action android:name="com.quickdroid.apps.QUERY"/>
            </intent-filter>
        </service>
        <receiver android:name=".apps.AppSyncStateManager">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <action android:name="android.intent.action.PACKAGE_CHANGED"/>
                <action android:name="android.intent.action.PACKAGE_REMOVED"/>
                <action android:name="android.intent.action.LOCALE_CHANGED"/>
                <data android:scheme="package"/>
            </intent-filter>
        </receiver>
        <provider android:authorities="com.quickdroid.favoriteitems.FavoriteItemsProvider" android:multiprocess="false" android:name=".favoriteitems.FavoriteItemsProvider"/>
        <receiver android:label="@string/appName" android:name=".SearchWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/search_widget_info"/>
        </receiver>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15"/></manifest>
