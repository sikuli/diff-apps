        <activity android:configChanges="orientation" android:exported="false" android:label="@string/details" android:launchMode="singleTop" android:name=".BugDetails" android:theme="@style/Theme.GreenDroid.Light.NoTitleBar"/>
        <activity android:exported="false" android:name=".BugDetails$SummaryActivity"/>
        <activity android:exported="false" android:name=".BugDetails$DetailsActivity"/>
        <activity android:exported="false" android:name=".BugDetails$CommentsActivity"/>
        <receiver android:enabled="true" android:exported="false" android:name="UpdateBroadcastReceiver"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15"/></manifest>
