<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.tdizz.sysmon" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application android:debuggable="true" android:icon="@drawable/task_manager" android:label="@string/app_name">
        <receiver android:exported="true" android:name=".traffic.TrafficReceiver">
            <intent-filter android:priority="1000">
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
        <activity android:label="@string/app_name" android:name=".SysMonActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="Clock" android:name=".clock.ClockActivity"/>
        <activity android:label="Contacts" android:name=".contacts.ContactActivity"/>
        <activity android:label="Data" android:name=".data.DataActivity"/>
        <activity android:label="Recorder" android:name=".recorder.RecorderActivity"/>
        <activity android:label="Traffic" android:name=".traffic.TrafficActivity"/>
        <activity android:label="Vibration" android:name=".vibration.VibrationActivity"/>
        <activity android:label="Wi-Fi" android:name=".wifi.WiFiActivity"/>
        <activity android:label="VPN" android:name=".vpn.VpnClientActivity"> android:configChanges="orientation|keyboardHidden" &gt;
</activity>
        <service android:name=".vpn.SysMonVpnService" android:permission="android.permission.BIND_VPN_SERVICE">
            <intent-filter>
                <action android:name="android.net.VpnService"/>
            </intent-filter>
        </service>
        <activity android:label="Voice" android:name=".voice.VoiceRecognitionSysMon"/>
        <activity android:label="Speech To Text" android:name=".speechtext.SpeechTextActivity"/>
    </application>
<uses-sdk android:minSdkVersion="15"/></manifest>
