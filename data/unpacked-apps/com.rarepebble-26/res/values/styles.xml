    </style>
    <style name="Theme.myLight" parent="@android:style/Theme.Light">
        <item name="android:autoCompleteTextViewStyle">@style/AutoCompleteTextViewLight</item>
    </style>
    <style name="CalculatorTextButton" parent="@android:style/Widget.Button">
        <item name="android:layout_width">65.0dip</item>
