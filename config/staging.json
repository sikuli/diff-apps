{
  "dbConfig": {
    "mongo": {
      "host": "localhost",
      "port": 27017,
      "db": "apps",
      "config": "./mongod.conf",
      "collections": [
        {
          "collection": "listings",
          "type": "listingsCollection",
          "description": "Listing details collection"
        }
      ]
    },
    "solr": {
      "host": "localhost",
      "port": 8983,
      "uiTagCollection": "sieveable-ui-tag",
      "uiTagH1Collection": "sieveable-ui-h1",
      "manifestCollection": "sieveable-manifest",
      "codeCollection": "sieveable-code"
    },
    "redis": {
      "config": "./redis.conf",
      "host": "localhost",
      "port": 6379
    }
  },
  "dataset": {
    "path": "../datasets/small",
    "keyName": "dataset:id",
    "indexKeyName": "dataset:index",
    "uiKeyName": "dataset:ui",
    "manifestKeyName": "dataset:manifest",
    "codeKeyName": "dataset:code"
  },
  "index": {
    "extractUITagDir": "../indexes/ui/tag",
    "extractUIH1Dir": "../indexes/ui/h1",
    "manifestDir": "../indexes/manifest"
  },
  "logger": {
    "error": {
      "path": "/var/tmp/sieveable-stage-error.log"
    }
  }
}