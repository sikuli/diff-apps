{
  "dbConfig": {
    "solr": {
      "host": "localhost",
      "port": 8983,
      "maxRows": 10000000,
      "listingCollection": "sieveable-listing",
      "uiTagCollection": "sieveable-ui-tag",
      "uiSuffixCollection": "sieveable-ui-suffix",
      "manifestCollection": "sieveable-manifest",
      "codeCollection": "sieveable-code"
    },
    "redis": {
      "config": "./redis.conf",
      "host": "localhost",
      "port": 6379
    }
  },
  "dataset": {
    "path": "../fixtures",
    "keyName": "dataset:id",
    "listingKeyName": "dataset:listing",
    "uiKeyName": "dataset:ui",
    "manifestKeyName": "dataset:manifest",
    "codeKeyName": "dataset:code"
  },
  "indexes": {
    "extractUITagDir": "../indexes/ui/tag",
    "extractUISuffixDir": "../indexes/ui/suffix",
    "extractManifestDir": "../indexes/manifest"
  },
  "results": {
    "maxUI": 10,
    "maxManifest": 10
  },
  "logger": {
    "error": {
      "path": "/var/tmp/sieveable-error.log"
    }
  }
}